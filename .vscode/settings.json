{
  "files.associations": {
    "vector": "cpp",
    "ostream": "cpp"
  },
  "vim.visualstar": true,
  "vim.hlsearch": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -O2 -std=c++14 -D_GLIBCXX_DEBUG $fileName && ./a.out",
  },
  "code-runner.saveFileBeforeRun": true,
}